host_name () {
unset name
while [[ ${name} = "" ]]; do
echo -ne "\033[1;37m Nuevo nombre del host : " && read name
tput cuu1 && tput dl1
done
hostnamectl set-hostname $name 
echo $name > name
if [ $(hostnamectl status | head -1  | awk '{print $3}') = "${name}" ]; then 
echo -e "\033[1;33m Host alterado corretamente !, reiniciar VPS"
else
echo -e "\033[1;33m Host no modificado!"
fi
return
}

cambiopass () {
[[ -e /root/name ]] && figlet -p -f smslant < /root/name | lolcat || echo -e "\033[7;49;35m    =====>>â–ºâ–º ğŸ² New ChumoGHğŸ’¥VPS ğŸ² â—„â—„<<=====      \033[0m"
echo -e "\033[1;37m  ã€  â˜… Reseller : $(cat < /bin/ejecutar/menu_credito) 2022 â˜…  ã€‘\033[0m"
msg -bar
echo -e ""
echo -e "${cor[3]} Esta herramienta cambia la contraseÃ±a de su servidor vps"
echo -e "${cor[3]} Esta contraseÃ±a es utilizada como usuario root"
echo -ne "\033[1;32m Desea Seguir? [S/N]: "; read x
[[ $x = @(n|N) ]] && fun_bar && return
#Inicia Procedimentos
echo -e "${cor[0]} Escriba su nueva contraseÃ±a"
read  -p " Nuevo passwd: " pass
(echo $pass; echo $pass)|passwd 2>/dev/null
sleep 1s
fun_bar
echo -e "${cor[3]} ContraseÃ±a cambiada con exito!"
echo -e "${cor[2]} Su contraseÃ±a ahora es : ${cor[4]}$pass"
return
}

