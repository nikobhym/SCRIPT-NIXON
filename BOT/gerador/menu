#!/bin/bash
export PATH=$PATH:/usr/sbin:/usr/local/sbin:/usr/local/bin:/usr/bin:/sbin:/bin:/usr/games
start_menu () {
source cabecalho
mf=$(free -h | grep Mem | sed 's/\s\+/,/g' | cut -d , -f4)
mb=$(free -h | grep Mem | sed 's/\s\+/,/g' | cut -d , -f6)
ml=$(free -h | grep Mem | sed 's/\s\+/,/g' | cut -d , -f7)
pp=$(cat /proc/cpuinfo | grep "model name" | uniq | awk -F ":" '{print $2}')
csss=$(cat /proc/cpuinfo | grep processor | wc -l)
usoo=$(grep 'cpu ' /proc/stat | awk '{usage=($2+$4)*100/($2+$4+$5)} END {print usage "%"}')
CPU=$(echo $usoo | awk '{printf("%d\n",$1 + 0.5)}')
##
opti=$(cat < /bin/ejecutar/val)
if [ "$opti" = "0" ]; then
_opti="\033[0;31m [OFF]"
else
_opti="\033[0;31m [\033[0;32mON\033[0;31m]"
fi
unset _usop _usor
_usor=$(printf '%-8s' "$(free -m | awk 'NR==2{printf "%.2f%%", $3*100/$2 }')")
_usop=$(printf '%-1s' "$(top -bn1 | awk '/Cpu/ { cpu = "" 100 - $8 "%" }; END { print cpu }')")

[[ -e /etc/bash.bashrc-bakup ]] && _autorun="\033[0;31m [\033[0;32mON\033[0;31m] " || _autorun="\033[0;31m [OFF]"
msg -bar #echo -e "${cor[1]} â–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ª ${cor[0]}"
#echo -e "${cor[2]} â€£ \033[1;37m${txt[3]} $_os ${cor[2]}â€£ \033[1;37mBase:\033[1;31m$modelo1 ${cor[2]}â€£ \033[1;37m${txt[346]}\033[1;31m $_core "
echo -e "\033[1;34m â€£\033[1;31m TOTAL:\033[1;32m "$(free -h | grep Mem | sed 's/\s\+/,/g' | cut -d , -f2) "\033[1;34m â€£ \033[1;31m Libre:\033[1;32m "$ram2 "\033[1;34m â€£ \033[1;31m Usada:\033[1;32m "$ram3
echo -e "\033[1;34m â€£\033[1;31m Uso RAM:\033[1;32m"$_usor "\033[1;34mâ€£\033[1;31m Uso CPU:\033[1;32m$_usop \033[1;34m â€£\033[1;31m Cache:\033[1;32m"$mb # $CPU"%"
msg -bar #echo -e "${cor[1]} â–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ªâ–ª ${cor[0]}"
echo -e "\033[0;35m [\033[0;36m1\033[0;35m]\033[0;31m â®${cor[3]} CONTROL USUARIOS (SSH/SSL/DROPBEAR/VMESS) "
echo -e "\033[0;35m [\033[0;36m2\033[0;35m]\033[0;31m â®${cor[3]} HERRAMIENTAS (\033[0;34mBad/VPN$_badvpn${cor[3]}) (\033[0;34mTCP-SPEED$_tcpspeed${cor[3]})"
echo -e "\033[0;35m [\033[0;36m3\033[0;35m]\033[0;31m â®\033[0;33m [!]${cor[3]} OPTIMIZAR VPS "
echo -e "\033[0;35m [\033[0;36m4\033[0;35m]\033[0;31m â®\033[0;33m [!]\033[0;31m DESINSTALAR SCRIPT"
echo -e "\033[0;35m [\033[0;36m6\033[0;35m]\033[0;31m â®${cor[3]} Cont/Limit DE USUARIOS $VERIFICAR_PID"
echo -e "\033[0;35m [\033[0;36m7\033[0;35m]\033[0;31m â® \e[3;32mHERRAMIENTAS BETA\e[0m \033[0;33m (#EXPERIMENTAL)"
echo -e "\033[0;35m [\033[0;36m8\033[0;35m]\033[0;31m â®${cor[3]} AUTOINICIAR SCRIPT $_autorun"
echo -e "\033[0;35m [\033[0;36m9\033[0;35m]\033[0;31m â®${cor[3]} INSTALADOR DE PROTOCOLOS"
msg -bar
echo -ne "\033[0;35m [\033[0;36m10\033[0;35m]\033[0;31m â®\033[0;33m [!] \033[1;32mACTUALIZAR${cor[3]}  |" && echo -e " \033[0;35m [\033[0;36m0\033[0;35m]\033[0;31m â® $(msg -bra "\033[1;41m[ Salir ]\e[0m")"
msg -bar 
}
start_menu
unset lang
[[ -e /etc/cgh/modulo/limiter ]] && lmt="\033[1;32m[ON] " || lmt="\033[1;31m[OFF]"
#FUNCION DE SELECCION
selection_fun () {
local selection="null"
local range
for((i=0; i<=$1; i++)); do range[$i]="$i "; done
while [[ ! $(echo ${range[*]}|grep -w "$selection") ]]; do
echo -ne "\033[1;37m Opcion: " >&2
read selection
tput cuu1 >&2 && tput dl1 >&2
done
echo $selection
}

